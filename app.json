{
  "expo": {
    "name": "Kaktus",
    "slug": "Kaktus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spc1.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "permissions": [
      "CAMERA",
      "AUDIO_RECORDING",
      "LOCATION",
      "NOTIFICATIONS",
      "MEDIA_LIBRARY",
      "WRITE_EXTERNAL_STORAGE"
    ]
  }
}
